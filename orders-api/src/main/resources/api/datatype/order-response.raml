#%RAML 1.0 DataType
type: object
properties:
  userID: string
  orderID: string
  orderStatus: 
    type: string
    enum: ['Processing']
  totalCost: string
  totalItemsOrdered: integer
  lineItems: 
    type: array
    items:
      type: object
      properties:
        itemID: integer
        description: string
        noOfItemsOrdered: integer
        